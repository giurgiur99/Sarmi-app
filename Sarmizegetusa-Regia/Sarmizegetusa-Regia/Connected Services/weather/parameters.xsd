                <xsd:enumeration value="LAMP_VISCAT" />
                <xsd:enumeration value="LAMP_CVIS" />
                <xsd:enumeration value="LAMP_CVISCAT" />
                <xsd:enumeration value="LAMP_CLD" />
                <xsd:enumeration value="LAMP_TP2" />
                <xsd:enumeration value="LAMP_TC2" />
